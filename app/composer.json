{
    "name": "symnext/app",
    "description": "A content management system using PHP, SQL, XML and XSLT.",
    "type": "project",
    "license": "None",
    "authors": [
        {
            "name": "Peter S",
            "email": "peter.s@mail.uk"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "symnext/symnext": "@dev",
        "symnext/symnext_admin_ui": "@dev",
        "vanilla/garden-cli": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../packages/symnext/symnext",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../packages/symnext/symnext_admin_ui",
            "options": {
                "symlink": true
            }
        }
    ]
}
